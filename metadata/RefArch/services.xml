<?xml version="1.0" encoding="UTF-8"?>
<services xmlns="http://www.demandware.com/xml/impex/services/2014-09-26">
    <service-credential service-credential-id="int_mercaux_api_service_creds">
        <url>https://api.mercaux.com/1.0/api</url>
        <user-id/>
        <password />
    </service-credential>

    <service-profile service-profile-id="int_mercaux_api_service_profile">
        <timeout-millis>0</timeout-millis>
        <rate-limit-enabled>false</rate-limit-enabled>
        <rate-limit-calls>0</rate-limit-calls>
        <rate-limit-millis>0</rate-limit-millis>
        <cb-enabled>false</cb-enabled>
        <cb-calls>0</cb-calls>
        <cb-millis>0</cb-millis>
    </service-profile>

    <service service-id="int_mercaux_api_service">
        <service-type>HTTP</service-type>
        <enabled>true</enabled>
        <log-prefix>mercaux_api</log-prefix>
        <comm-log-enabled>false</comm-log-enabled>
        <force-prd-enabled>true</force-prd-enabled>
        <mock-mode-enabled>false</mock-mode-enabled>
        <profile-id>int_mercaux_api_service_profile</profile-id>
        <credential-id>int_mercaux_api_service_creds</credential-id>
    </service>

</services>
